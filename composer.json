{
    "name": "pongocms/cms",
    "description": "The back-end package for PongoCMS v2",
    "homepage": "https://pongocms.com",
    "keywords": ["laravel", "cms", "pongo", "bootstrap", "jquery"],
    "time": "2014-07-01",
    "license": "MIT",
    "authors": [
        {
            "name": "Fabio Fumis",
            "email": "f.fumis@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "4.1.*",
        "prologue/alerts": "dev-master",
        "intervention/image": "2.*",
        "intervention/imagecache": "dev-master",
        "intervention/helper": "dev-master",
        "laracasts/utilities": "dev-master"
    },
    "require-dev": {
        "fzaninotto/faker": "dev-master"
    },
    "autoload": {
        "classmap": [
            "src/migrations",
            "src/seeds"
        ],
        "psr-0": {
            "Pongo\\Cms\\": "src/"
        }
    },
    "minimum-stability": "dev"
}
